{
  "name": "floralink-backend",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "babel-node  dist/index.js",
    "start:prod": "cross-env NODE_ENV=production nodemon --exec babel-node ./src/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "build": "babel src --out-dir dist --source-maps inline --copy-files",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "reset": "npm run down && npm run migrate && npm run seed",
    "resetDev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo:all && cross-env NODE_ENV=development npx sequelize-cli db:migrate && cross-env NODE_ENV=development npx sequelize-cli db:seed:all",
    "resetProd": "cross-env NODE_ENV=production npx sequelize-cli db:migrate:undo:all && cross-env NODE_ENV=production npx sequelize-cli db:migrate && cross-env NODE_ENV=production npx sequelize-cli db:seed:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.14.5",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.7.9",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-istanbul": "^6.1.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "chart.js": "^4.4.8",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.4.1",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "http": "0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.9.14",
